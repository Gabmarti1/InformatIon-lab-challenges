    "SELECT et.email\n",
    "FROM public.data_table as dt\n",
    "JOIN public.email_table as et ON dt.join_id = et.join_id\n",
    "WHERE dt.column_1 % 2 = 0\n",
    "AND dt.column_2 < dt.column_1\n",
    "AND dt.column_3 % 10 = 1"
   ]
